{
  "presets": ["es2015", "react", "stage-1"],
  "plugins": [
    "add-module-exports",
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-react-jsx",
    [
      "import",
      {
        "style": true,
        "libraryDirectory": "lib",
        "libraryName": "antd",
        "camel2DashComponentName": true
      }
    ]
  ],
  "env":{
    "development":{
      "presets": ["react-hmre"]
    }
  }
}